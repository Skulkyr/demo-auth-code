<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.20.xsd">


<changeSet id="001-create-user-sequence" author="Alexey">
        <createSequence
                sequenceName="user_seq"
                startValue="1"
                incrementBy="50" />
    </changeSet>

    <changeSet id="002-create-confirmation_codes-sequence" author="Alexey">
        <createSequence
                sequenceName="confirmation_code_seq"
                startValue="1"
                incrementBy="50" />
    </changeSet>

</databaseChangeLog>